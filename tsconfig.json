{
  "compilerOptions": {
    "module": "commonjs",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "outDir": "./bin",
    "baseUrl": "./",
    "strict": true,
        "incremental": true,
        "listEmittedFiles": true,
        "pretty": true,
        "sourceMap": false,
        "declaration": false
  },
  "exclude": ["node_modules"]
}
